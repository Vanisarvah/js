<script>
var fruites = ["apple", "banana", "orange","pineapple","mango"];
document.getElementById("demo3").innerHTML = fruites;

function myFunction() {
    fruites.sort();
    fruites.reverse();
    document.getElementById("demo3").innerHTML = fruites;
}
</script>

